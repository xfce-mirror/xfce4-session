msgstr "_Surt"

#: ../xfce4-session/xfsm-manager.c:1317
msgid "Shutdown Failed"
msgstr "Ha fallat l'apagada"

#: ../xfce4-session/xfsm-manager.c:1320
msgid "Failed to suspend session"
msgstr "Ha fallat la suspensió de la sessió"

#: ../xfce4-session/xfsm-manager.c:1322
msgid "Failed to hibernate session"
msgstr "Ha fallat la hibernació de la sessió"

#: ../xfce4-session/xfsm-manager.c:1324
msgid "Failed to hybrid sleep session"
msgstr "Ha fallat la suspensió híbrida de la sessió"

#: ../xfce4-session/xfsm-manager.c:1325
msgid "Failed to switch user"
msgstr "Ha fallat el canvi d'usuari"

#: ../xfce4-session/xfsm-manager.c:1647
#, c-format
msgid "Can only terminate clients when in the idle state"
msgstr "Només es poden finalitzar els clients quan l'estat és inactiu"

#: ../xfce4-session/xfsm-manager.c:2355
msgid "Session manager must be in idle state when requesting a checkpoint"
msgstr "El gestor de sessions ha d'estar en estat inactiu quan se sol·licita un punt de control"

#: ../xfce4-session/xfsm-manager.c:2425 ../xfce4-session/xfsm-manager.c:2445
msgid "Session manager must be in idle state when requesting a shutdown"
msgstr "El gestor de sessions ha d'estar en estat inactiu quan se sol·licita una apagada"

#: ../xfce4-session/xfsm-manager.c:2482
msgid "Session manager must be in idle state when requesting a restart"
msgstr "El gestor de sessions ha d'estar en estat de repòs quan se sol·licita un reinici"

#: ../xfce4-session/xfsm-manager.c:2518 ../xfce4-session/xfsm-manager.c:2557
#: ../xfce4-session/xfsm-manager.c:2595
msgid "Session manager must be in idle state when requesting a sleep"
msgstr "El gestor de sessions ha d'estar en estat de repòs quan se sol·licita una aturada temporal"

#: ../xfce4-session/xfsm-manager.c:2722
msgid "Session manager must be in idle state when requesting a user switch"
msgstr "El gestor de sessions ha d'estar en estat inactiu quan se sol·licita un canvi d'usuari"

